{
  "name": "jasmine_poc",
  "version": "0.0.1",
  "main": "src/jasmine.js",
  "scripts": {
    "start": "node server/index",
    "watch": "gulp",
    "dev": "npm run build-dist && concurrently \"nodemon ./server/index --watch server\" \"gulp\"",
    "build-dist": "gulp server.compile_tsc",
    "test-server": "npm run build-dist && jasmine --config=jasmine.json",
    "coverage-server": "npm run build-dist && npm run coverage && npm run mapping-coverage",
    "coverage": "ts-node node_modules/istanbul/lib/cli.js cover --include-all-sources true --root \"dist\" -x \"*_test.ts\" node_modules/jasmine/bin/jasmine.js -- JASMINE_CONFIG_PATH=jasmine.json",
    "mapping-coverage": "./node_modules/.bin/remap-istanbul -i coverage/coverage.json -o coverage/html-report -t html"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.11",
    "@types/express": "^4.0.37",
    "@types/helmet": "0.0.36",
    "@types/lodash": "^4.14.74",
    "@types/mongoose": "^4.7.21",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.28",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-es2016": "^6.0.11",
    "babel-preset-stage-0": "^6.1.18",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.7.2",
    "bluebird": "^3.3.4",
    "body-parser": "^1.5.0",
    "compression": "^1.6.2",
    "express": "~4.14.0",
    "express-content-length-validator": "1.0.0",
    "helmet": "^2.1.1",
    "lodash": "^4.6.1",
    "mongoose": "^4.4.7",
    "morgan": "^1.5.2",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "ts-node": "^3.3.0",
    "@types/jasmine": "^2.6.0",
    "aliv": "^1.7.2",
    "babili": "0.0.6",
    "browser-sync": "^2.5.2",
    "concurrently": "^2.0.0",
    "del": "^2.2.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^2.0.3",
    "gulp-concat": "^2.4.3",
    "gulp-coveralls": "^0.1.3",
    "gulp-htmlmin": "^2.0.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-rename": "^1.2.0",
    "gulp-rev-append": "^0.1.6",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-typescript": "~3.1.4",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.3",
    "istanbul": "^0.4.4",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.2.0",
    "nodemon": "^1.9.2",
    "protractor": "^3.1.0",
    "remap-istanbul": "^0.9.5",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5"
  },
